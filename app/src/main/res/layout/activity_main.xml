<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    >

  <TextView
      android:id="@+id/click_normal"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="@string/click_normal"
      android:padding="20.0dp"
      android:textSize="16.0dp"
      android:textColor="@color/ct_1"
      tools:ignore="SpUsage"
      />

  <View
      android:layout_width="match_parent"
      android:layout_height="0.4dp"
      android:background="@color/line"
      />

  <TextView
      android:id="@+id/click_cell_class"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="@string/click_cell_class"
      android:padding="20.0dp"
      android:textSize="16.0dp"
      android:textColor="@color/ct_1"
      tools:ignore="SpUsage"
      />

  <View
      android:layout_width="match_parent"
      android:layout_height="0.4dp"
      android:background="@color/line"
      />

  <TextView
      android:id="@+id/click_cell_integer"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="@string/click_cell_integer"
      android:padding="20.0dp"
      android:textSize="16.0dp"
      android:textColor="@color/ct_1"
      tools:ignore="SpUsage"
      />

  <View
      android:layout_width="match_parent"
      android:layout_height="0.4dp"
      android:background="@color/line"
      />

</LinearLayout>